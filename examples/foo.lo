#c.'#include <stdio.h>'

// TODO return type
pub fun ini_decode(s string) {
	lines := s.split("\n")
	for i := 0; i < lines.length; i += 1 {
		line := lines[i]
		if line.length == 0 || line.starts_with(";") {
			continue
		}
		#ts.'console.log(line)'
		#c.'printf("%s\n", line);'
	}
}

@main
fun my_main() {
	s := "[section]
; my comment
key = value"
	ini_decode(s)
}
